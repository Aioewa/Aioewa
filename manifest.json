{
  "name": "__MSG_extName__",
  "short_name": "Aioewa",
  "manifest_version": 3,
  "version": "1.0.0",
  "version_name": "1-beta",
  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "author": "STIO",
  "background": {
    "service_worker": "/file/background.js",
    "type": "module"
  },
  "minimum_chrome_version": "92",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_icon": "/asset/icon/icon48.png",
    "default_popup": "/asset/html/popup.html",
    "default_title": "Aioewa"
  },
  "icons": {
    "16": "/asset/icon/icon16.png",
    "48": "/asset/icon/icon48.png",
    "128": "/asset/icon/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start",
      "js": [
        "api/inject.js"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/addon/*",
        "/api/*",
        "/asset/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "info@stio.studio",
      "strict_min_version": "42.0"
    }
  },
  "externally_connectable": {
    "matches": [
      "<all_urls>"
    ]
  },
  "options_ui": {
    "page": "/asset/html/settings.html",
    "open_in_tab": true
  },
  "homepage_url": "https://aioewa.stio.studio",
  "sidebar_action": {
    "default_icon": "/asset/icon/icon128.png",
    "default_panel": "/asset/html/settings.html",
    "default_panel_width": 604,
    "default_title": "",
    "maximum_panel_width": 1000,
    "minimum_panel_width": 400,
    "panel_width": 604
  }
}