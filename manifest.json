{
  "name": "__MSG_extName__",
  "short_name": "Aioewa",
  "manifest_version": 3,
  "version": "1.0.0",
  "version_name": "1-beta",
  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "author": "STIO",
  "background": {
    "service_worker": "/file/background/main.js"
  },
  "permissions": [
    "storage"
  ],
  "action": {
    "default_icon": "/asset/icon/icon48.png",
    "default_popup": "/asset/html/popup.html",
    "default_title": "Aioewa"
  },
  "icons": {
    "16": "/asset/icon/icon16.png",
    "48": "/asset/icon/icon48.png",
    "128": "/asset/icon/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_start",
      "js": [
        "file/inject/style.js",
        "file/inject/script.js"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/addon/*",
        "/api/*",
        "/asset/*"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "host_permissions": [
    "*://*/"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "info@stio.studio",
      "strict_min_version": "42.0"
    }
  },
  "externally_connectable": {
    "matches": [
      "*://*/*"
    ]
  },
  "options_ui": {
    "page": "file/settings/index.html",
    "open_in_tab": true
  },
  "homepage_url": "https://aioewa.stio.studio"
}